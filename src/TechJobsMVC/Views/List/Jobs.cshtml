<h1>@ViewBag.title</h1>
@ViewBag.columns = ListController.columnChoices;
<h3>@ViewBag.jobs.Count Result(s)</h3>
<table>

@foreach(Dictionary<string, string> job in ViewBag.jobs)
{
@foreach (KeyValuePair<string, string> item in job)
{
    <tr >
        <td><a asp-controller="List" asp-action="Values" asp-route-column="@item.Key" asp-route-value="@item.Value">@item.Key @item.Value</a></td>
    </tr>  
}
}
</table>
